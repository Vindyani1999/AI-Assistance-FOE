{
  "name": "agent-graph-system",
  "version": "1.0.0",
  "description": "Intelligent SQL-agent Q&A and RAG System for Chatting with Multiple Databases",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "install:all": "npm install && cd frontend && npm install",
    "dev:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "dev:backend": "cd backend && python main.py fastapi",
    "dev:gradio": "cd backend && python main.py gradio"
  },
  "keywords": [
    "ai",
    "agents",
    "rag",
    "sql",
    "langchain",
    "openai",
    "chatbot"
  ],
  "author": "AgentGraph Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
